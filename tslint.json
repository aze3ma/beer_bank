{
	"extends": [
		"tslint:recommended",
		"tslint-react",
		"tslint-config-prettier"
	],
	"rulesDirectory": [
		"tslint-plugin-prettier"

	],
	"rules": {
		"prettier": true,
		"interface-name": false,
		"ordered-imports": false,
		"member-access": [true, "check-accessor"],

	},
	"linterOptions": {
		"exclude": ["./src/serviceWorker.ts"]
	}
}
